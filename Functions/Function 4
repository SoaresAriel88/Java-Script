//Verificação de uma palavra é um palindromo//

function palindromo(palavra){
    var palavra = 'ana'
   const teste = palavra.split("").reverse().join("");
   if (palavra === teste){
    console.log("Essa palavra é um palindromo");
    return true
   }  else{
    console.log("Essa palavra não é um palindromo")
   } return false;
    
}
console.log(palindromo())
